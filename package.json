{
  "name": "qan-app",
  "version": "1.0.6",
  "description": "Query Analytics Application",
  "main": "app/main.ts",
  "dependencies": {
    "@angular/common": "~2.2.1",
    "@angular/compiler": "~2.2.1",
    "@angular/compiler-cli": "^2.2.1",
    "@angular/core": "~2.2.1",
    "@angular/forms": "~2.2.1",
    "@angular/http": "~2.2.1",
    "@angular/platform-browser": "~2.2.1",
    "@angular/platform-browser-dynamic": "~2.2.1",
    "@angular/platform-server": "^2.2.1",
    "@angular/router": "~3.2.1",
    "@angular/upgrade": "~2.2.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.13",
    "angular-in-memory-web-api": "~0.1.16",
    "bootstrap": "^4.0.0-alpha.5",
    "core-js": "^2.4.1",
    "d3": "^4.3.0",
    "font-awesome": "^4.7.0",
    "highlightjs": "^9.8.0",
    "moment": "^2.16.0",
    "moment-timezone": "^0.5.9",
    "numeral": "^1.5.5",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.12",
    "systemjs": "0.19.41",
    "vkbeautify": "^0.99.1",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/d3": "^4.3.0",
    "@types/node": "^6.0.49",
    "@types/numeral": "0.0.18",
    "browserify": "^13.1.1",
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "rollup": "^0.36.3",
    "tslint": "^4.0.1",
    "typescript": "^2.0.10",
    "typings": "^2.0.0",
    "uglifyjs": "^2.4.10",
    "webpack": "^2.1.0-beta.27"
  },
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "postinstall": "typings install",
    "typings": "typings",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rm -rf dist/* && rm -rf app/*.ngfactory.ts && cd compiled && find . ! -name 'main-ngc.ts' -type f -exec rm -f {} + && cd ..",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json && cp -r app/ compiled/",
    "build": "tsc -p tsconfig-aot.json",
    "rollup": "./node_modules/.bin/rollup -f iife -c rollup.config.js -o dist/bundle.es2015.js",
    "es5": "tsc --allowJs --target es5 dist/bundle.es2015.js --out dist/bundle.js",
    "minify": "./node_modules/.bin/uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "build_prod": "npm run clean && npm run ngc && npm run build && npm run rollup && npm run es5 && npm run minify",
    "build:my": "npm run build && ./node_modules/.bin/browserify -s main dist/main.js > dist/bundle.js && npm run es5 && npm run minify",
    "build:aot": "./node_modules/.bin/ngc -p tsconfig-aot.json && ./node_modules/.bin/rollup -c rollup-config.js",
    "lite:aot": "lite-server -c aot/bs-config.json"
  },
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/percona/qan-app.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/percona/qan-app/issues"
  },
  "homepage": "https://github.com/percona/qan-app#readme"
}
